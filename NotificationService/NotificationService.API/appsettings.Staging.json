{
  "ListeningPorts": {
    "Api": 80
  },
  "ConnectionStrings": {
    "SqlServer": null
  },
  "EventBus": {
    "AzureEventBus": {
      "TopicName": "eszop-staging-event-bus-topic",
      "SubscriptionName": "eszop-staging-event-bus-notification-sub"
    },
    "UseAzureEventBus": true
  },
  "NotificationSettings": {
    "CleanupJobCronPattern": "0 0 * * *", // Every midnight
    "ExpirationInMinutes": "43200"
  },
  "JwtConfig": {
    "Issuer": "Identity.API",
    "Audience": "Identity.API.Audience",
    "AccessTokenSecretKey": ""
  },
  "IsScaledOutService": true
}
